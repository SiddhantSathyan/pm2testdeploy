{
    "$schema": "http://json.schemastore.org/pm2-ecosystem",
    "apps": [
        {
            "name": "decision-module",
            "script": "./dist/src/index.js",
            "instances": 1,
            "exec_mode": "fork",
            "watch": false,
            "env": {
                "NODE_ENV": "production"
            },
            "env_production": {
                "PORT": "3000"
            },
            "env_staging": {
                "PORT": "3100"
            }
        }
    ],
    "deploy": {
        "production": {
            "host": "192.168.0.101",
            "ref": "origin/main",
            "repo": "https://github.com/SiddhantSathyan/pm2testdeploy.git",
            "path": "C:/deploy/decision-module-production",
            "user": "CSE013"
        },
        "staging": {
            "host": "192.168.0.101",
            "ref": "origin/main",
            "repo": "https://github.com/SiddhantSathyan/pm2testdeploy.git",
            "path": "C:/deploy/decision-module-staging",
            "user": "CSE013"
        }
    }
}